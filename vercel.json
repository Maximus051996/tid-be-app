{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs/(.*)",
            "dest": "/public/swagger/$1"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}